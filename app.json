{
  "expo": {
    "name": "oddly",
    "slug": "oddly",
    "version": "1.0.0",
    "scheme": "oddly",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-location"
    ],
    "permissions": {
      "location": {
        "always": "Allow $(PRODUCT_NAME) to use your location."
      }
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7b90e5ea-f1b1-4406-82f9-f9c91d568da6"
      }
    },
    "owner": "sonkey"
  }
}
